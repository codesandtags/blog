    "gatsby-plugin-mailchimp": "^5.1.2",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-netlify-cms": "^4.1.40",
    "gh-pages": "^2.2.0",
    "netlify-cms-app": "^2.11.23",
